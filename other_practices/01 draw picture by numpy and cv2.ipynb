   "source": [
    "import numpy \n",
    "import cv2\n",
    "\n",
    "img = numpy.zeros([300, 500, 3])\n",
    "\n",
    "img[:, :, 0] = numpy.ones([300, 500])*64/255.0\n",
    "img[:, :, 1] = numpy.ones([300, 500])*128/255.0\n",
    "img[:, :, 2] = numpy.ones([300, 500])*192/255.0\n",
    "\n",
    "cv2.imshow(\"image\", img)\n",
    "cv2.waitKey()"
   ]
  }
